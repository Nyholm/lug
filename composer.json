{
    "name": "php-lug/lug",
    "license": "MIT",
    "type": "project",
    "description": "Lug framework",
    "autoload": {
        "psr-4": { "Lug\\": "src/" }
    },
    "autoload-dev": {
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": "^5.6|^7.0",
        "symfony/symfony": "^2.8"
    },
    "require-dev": {
        "symfony/monolog-bundle": "^2.4",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.4",
        "doctrine/doctrine-fixtures-bundle": "^2.0",
        "doctrine/mongodb-odm-bundle": "3.1.x-dev",
        "stof/doctrine-extensions-bundle": "^1.1",
        "friendsofsymfony/rest-bundle": "dev-lug",
        "jms/serializer-bundle": "^1.0",
        "willdurand/hateoas-bundle": "^1.0",
        "a2lix/translation-form-bundle": "^2.0",
        "white-october/pagerfanta-bundle": "^1.0",
        "knplabs/knp-menu-bundle": "^2.1",
        "sensio/distribution-bundle": "^5.0",
        "incenteev/composer-parameter-handler": "^2.0",
        "fabpot/php-cs-fixer": "^1.0",
        "phpunit/phpunit": "^5.0",
        "symfony/phpunit-bridge": "^3.0",
        "behat/symfony2-extension": "^2.0",
        "behat/mink-extension": "^2.0",
        "behat/mink-browserkit-driver": "^1.0",
        "behat/mink-selenium2-driver": "^1.0",
        "php-http/discovery": "^0.7",
        "php-http/message": "^0.2",
        "php-http/guzzle6-adapter": "^0.4",
        "coduo/php-matcher": "^1.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/egeloen/FOSRestBundle.git"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
